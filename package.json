{
  "name": "compact-img",
  "version": "0.1.0",
  "description": "Lossy compress images in batch",
  "author": "Beeno Tung <https://beeno-tung.surge.sh>",
  "license": "BSD-2-Clause",
  "bin": {
    "compact-img": "./compact-img"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:client": "parcel build --out-dir public src/index.html",
    "build:server": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build:client && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "public",
    "compact-img"
  ],
  "dependencies": {
    "@beenotung/tslib": "^14.38.4",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "body-parser": "^1.19.0",
    "open": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/compact-img.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/beenotung/compact-img/issues"
  },
  "homepage": "https://github.com/beenotung/compact-img#readme"
}
